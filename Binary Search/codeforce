#include <bits/stdc++.h>
    using namespace std;
    #define int long long
 
    int closetoleft(vector<int>A, int target){
        int l=0, h=A.size()-1,m;
        while(l<=h){
            m=(l+h)/2;
            if(target>=A[m]) l=m+1;
            else h=m-1;
        }
        return h+1;
    } 
    signed main() {
        int n,k;
        cin>>n>>k;
        vector<int> A(n);
        vector<int> K(k);
        for(int i=0; i<n; i++){
            cin>>A[i];
            
        }
        for(int i=0; i<k; i++){
            cin>>K[i];
            
        }
        for(int i=0; i<k; i++){
            cout<<closetoleft(A, K[i]);
        }
        return 0;
    }
